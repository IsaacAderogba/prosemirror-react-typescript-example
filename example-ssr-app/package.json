{
  "name": "example-ssr-app",
  "version": "0.1.0",
  "homepage": "https://teemukoivisto.github.io/prosemirror-react-typescript-example/",
  "scripts": {
    "dev": "nodemon --watch server-dist server-dist/index.js",
    "lint": "eslint ./server/**/* ./client/**/*",
    "lint:fix": "eslint ./server/**/* --fix",
    "test": "exit 0",
    "build": "npm run build:server && npm run build:client",
    "build:server": "rollup -c rollup.config.server.js",
    "build:client": "rollup -c rollup.config.client.js",
    "watch:server": "rollup -c rollup.config.server.js -w",
    "watch:client": "rollup -c rollup.config.client.js -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash.debounce": "^4.0.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "16.10.1",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-icons": "3.0.0",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.3.0",
    "@types/styled-components": "5.1.14",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "6.1.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "nodemon": "^2.0.13",
    "rollup": "^2.57.0",
    "rollup-plugin-inject-process-env": "1.3.1",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@pm-react-example/atlassian": "*",
    "@pm-react-example/full": "*",
    "@pm-react-example/minimal": "*",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "4.2.0",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "styled-components": "5.3.1",
    "winston": "^3.3.3"
  }
