    "@types/morgan": "^1.9.2",
    "@types/node": "14.14.17",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-icons": "3.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "5.1.7",
